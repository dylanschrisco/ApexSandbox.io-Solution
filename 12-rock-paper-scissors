public Integer rockPaperScissors(String player1, String player2) {
    //code here
    if (player1 == 'rock' && player2 == 'rock')
        return 0;
    else if (player1 == 'rock' && player 2 == 'paper')
        return 2;
    else if (player1 == 'rock' && player 2 == 'scissors')
        return 1;
    else if (player1 == 'paper' && player 2 == 'paper')
        return 0;
    else if (player1 == 'paper' && player 2 == 'scissors')
        return 2;
    else
        return 0;
}